import java.util.Scanner;


public class Ex01 {
	public static void main(String[] args) {
		
		Scanner sc= new Scanner(System.in);

			String str=sc.nextLine();
			
			
			int[] arr= new int[5]; 
			char b;
			for(int i=0; i<arr.length; i++) {
				b=str.charAt(i);
				System.out.println(b);
			
			}
		
			
			
				
			
			
			
			
			}
		
	
}
