import java.util.Scanner;

class Factorial{
	
	void mul(){
	Scanner sc=new Scanner(System.in);
	
	int pure=sc.nextInt();	
	
	while(pure>0) {

		if(pure==1) {
			break;
			
		}else {
			
			
		}
		pure--;
		
		
		System.out.print(pure+1+"*");
		
		
		}
	}
	
}

public class Ex02 {
	
	
	public static void main(String[] args) {
		
	
		Factorial fc1= new Factorial();
		
		
		fc1.mul();
		
		
	
		}
	}

