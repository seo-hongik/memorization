	int pure=sc.nextInt()+1;	
		int j;
		while(pure>0) {
			
			if(pure==1) {
				
				break;
				
				
				
				
			}else {
			
			System.out.print(pure+"*");
			
			
			}
			int k= pure--;
		}
		
	
