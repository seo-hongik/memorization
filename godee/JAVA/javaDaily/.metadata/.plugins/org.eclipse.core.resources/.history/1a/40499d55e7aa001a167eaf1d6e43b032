import java.util.Scanner;


public class Ex01 {
	public static void main(String[] args) {
		
		Scanner sc= new Scanner(System.in);

			
			
			
			int[] arr= new int[5]; 
			char ch1, ch2, ch3;
			for(int i=0; i<arr.length; i++) {
				String str=sc.nextLine();
				ch1=str.charAt(0);
				ch2=str.charAt(1);
				ch3=str.charAt(2);
				System.out.println(ch1);
						
			}
			
			
			
				
			
			
			
			
			}
		
	
}
