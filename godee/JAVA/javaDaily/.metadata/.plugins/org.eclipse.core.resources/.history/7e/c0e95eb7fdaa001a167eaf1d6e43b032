import java.util.Scanner;

public class Ex02 {
	
	
	public static void main(String[] args) {
		
	Scanner sc=new Scanner(System.in);
	
	
	int pure=sc.nextInt();	
		
		while(pure>0) {
			
			if(pure==1) {
				break;
				
			}
			pure--;
			
			int factorial=pure+1;
			System.out.print(factorial+"*");
			
			
		}
		System.out.print(pure*pure--);
	
	}
	}

