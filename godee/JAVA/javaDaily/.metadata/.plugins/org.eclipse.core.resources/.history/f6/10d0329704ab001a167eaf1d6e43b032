import java.util.Scanner;

public class Ex02 {
	
	
	public static void main(String[] args) {
		
	Scanner sc=new Scanner(System.in);
	
	
	int pure=sc.nextInt()+1;	
		
		while(pure>0) {
			pure--;
			if(pure==1) {
				break;
				
			}else {
				
				
			}
			
			
			
			System.out.print(pure+"*");
			
			
		}
		
	
	}
	}

