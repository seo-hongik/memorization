
public class Ex09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i;
		int L;
		int J;
		int K;
		i = 0;
		J = 1;
		L = -1;
		K = -1;
		do {
			i = i + 1;
			J = J + i;
			L = -1*(L);
			K = K +(L*J);
		} while(i < 19);
		System.out.println("i =" + i);
		System.out.println("L =" + L);
		System.out.println("J =" + J);
		System.out.println("K = " + K);
	}

}
