
public class Ex06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		double i =0;
		double J=0;
		System.out.println(5/2);
		System.out.println((int) 5/2);
		do {
			i++;
			if((int)(i/2) == i/2) {
				J = J + (i/(i+1));
			} else {
				J = J - (i/(i+1));
			}
		} while(i<99);
		System.out.printf("J=%f ", (double)J);
	} 

}
