package ledTV;

class TV {
	String color;
	boolean power;
	int channel;
	
	void power() {
		power=!power;
	}
	
	void channelUp() {
		++channel;
	}
	
	void channelDown() {
		--channel;
	}
}

public class TVTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		TV[] tvs = new TV[2];
	}

}
