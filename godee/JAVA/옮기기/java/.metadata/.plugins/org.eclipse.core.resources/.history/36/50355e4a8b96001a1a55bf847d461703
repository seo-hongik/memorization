class VarargsSample {
	int getMax(int...nums) {
		int max = 0;
		for(int i=0;i<nums.length;i++) {
			if(i==0) {
				max = nums[0];
			} else if (max<nums[i]) {
				max = nums[i];
			}
		}
		return max;
	}
}

public class AloneTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	}

}
