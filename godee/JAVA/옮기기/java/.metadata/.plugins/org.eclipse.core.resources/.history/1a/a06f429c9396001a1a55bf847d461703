class Animal2 {
	String name;
	int age;
	
	void printPet() {
		System.out.println("이름 : " + name);
		System.out.println("나이 : " + age);
	}	
}

class Dog extends Animal2 {
	String variety;
	
	void printPet() {
		super.printPet();
		System.out.println("종류 : " + variety);
	}
}
public class Pet {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Dog dog = new Dog();
		dog.name = "똑똑이";
		dog.age = 5;
		dog.variety = "말티즈";
		dog.printPet();
	}

}
