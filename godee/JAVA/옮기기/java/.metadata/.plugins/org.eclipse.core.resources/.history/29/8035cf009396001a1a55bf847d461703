class Animal2 {
	String name;
	int age;
	
	void printPet() {
		System.out.println("이름 : " + name);
		System.out.println("나이 : " + age);
	}	
}

class Cat extends Animal2 {
	String variety;
	
	void printPet() {
		super.printPet();
		System.out.println("종류 : " + variety);
	}
}
