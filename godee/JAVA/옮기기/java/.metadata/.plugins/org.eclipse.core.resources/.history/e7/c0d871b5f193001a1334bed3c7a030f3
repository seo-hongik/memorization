import java.util.Scanner;

public class Ex21 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[][] words = {
				{"chari", "의자"},
				{"computer", "컴퓨터"},
				{"apple", "사과"}
		};
		
		Scanner sc = new Scanner(System.in);
		
		for(int i=0; i<words.length; i++) {
			System.out.println(words[i]);
		}
		
	}

}
