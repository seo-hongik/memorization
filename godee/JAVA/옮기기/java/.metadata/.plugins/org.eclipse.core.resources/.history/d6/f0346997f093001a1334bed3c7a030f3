
public class Ex20 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		int[][] score1 = new int[5][3];
		
		int[][] score = new int[5][];
		System.out.println(score.length);
		score[0] = new int[3];
		System.out.println(score[0].length);
		
		score[1] = new int[3];
		score[2] = new int[1];
		score[3] = new int[5];
		score[4] = new int[4];
		score[5] = new int[5];
		score[6] = new int[7];
		System.out.println(score.length);
	}

}
