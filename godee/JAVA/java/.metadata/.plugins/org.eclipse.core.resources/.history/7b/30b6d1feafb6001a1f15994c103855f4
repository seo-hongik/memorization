//패키지 이름이 kakaoGame입니다.
package kakaoGame;

import java.util.Scanner;

//클래스 이름이 kakaoGameMain 입니다.
public class KakaoGameMain {

	//main()메서드가 생성되었습니다.
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc=new Scanner(System.in);
		
		Charater charater=null;
		PlayGameEngine pgEngine=null;
		
		System.out.println("=====KAKAO GAME=====");
		System.out.println("원하는 캐릭터를 생성하세요");
		System.out.println("1.라이언 2.어피치 3.무지");
		int num=sc.nextInt();
		
		switch(num) {
		case 1://라이언 생성
			charater=new Ryan();
			break;
		case 2://어피지 생성
			charater=new Apeach();
			break;

		case 3://무지 생성
			charater=new Muzi();
			break;
		
		
		}
		
		

	}

}
