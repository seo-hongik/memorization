//package이름이 polymorphismTest입니다.
package downCastingTest;


//상퀴 클래스 Animal를 정의합니다.

class Animal {
	
	
	//메서드(맴버 함수를) 정의합니다.
	
	public void move() {
		
		System.out.println("동물이 움직입니다.");
		
	}
	
}

//Animal클래스를 상속 받은 Human 클래스를 정의합니다. 


class Human extends Animal{
	
	
	
	
	
	
}

