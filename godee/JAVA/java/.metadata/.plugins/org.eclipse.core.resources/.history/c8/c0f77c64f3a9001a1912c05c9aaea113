package sec02;

class Car{
	
	private int num;
	private double gas;
	
	public void show() {
		System.out.println("차량 번호는"+num+"입니다.");
		System.out.println("연료량은"+gas+"입니다.");
	}
	
	
	
}

public class Sample02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Car car1=new Car();
		
		car1.show();
		
	}

}
