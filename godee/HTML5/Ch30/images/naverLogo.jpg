<script type="text/javascript">
    (function(){
        var id = "naver-splugin-sdk", t = new Date(), yyyy = t.getFullYear(), mm = t.getMonth() + 1, dd = t.getDate();
        var s = document.createElement("script"); s.id = id; s.type = "text/javascript", s.charset = "utf-8", s.async = false;
        s.src = ("https://ssl.pstatic.net/spi/js/release/ko_KR/splugin.js?" + yyyy + (mm < 10 ? '0' + mm : mm) + (dd < 10 ? '0' + dd : dd));
        var d = document.getElementsByTagName('head')[0]; d.appendChild(s, d);
    })();

    $(document).ready(function(){
        $(window).scroll(function(){
            var y = $(this).scrollTop();
            if(y >= 100){
                $(".bt_area").fadeIn();
            } else {
                $(".bt_area").fadeOut();
            }
        });

        $.ajax({
            type: "get",
            url: "/getContentSeqList",
            dataType: "json",
            data : { "logoType" : ""},
            success : function(data){
                var seq = 1096;
                var index = data.indexOf(seq+"");
                var prevIndex = index - 1 ;
                var nextIndex = index + 1;
                var prevSeq = -1;
                var nextSeq = -1;

                if( data.length !== nextIndex){
                    nextSeq = data[nextIndex];
                }

                if ( prevIndex > -1 ){
                    prevSeq = data[prevIndex];
                }

                if(nextSeq > 0){
                    $(".page_btn_box .page_btn").eq(1).removeClass("disable");
                    if(nextSeq >= 1000){
                        $(".page_btn_box .page_btn").eq(1).attr("href", "/detail?seq="+nextSeq+"&logoType=")
                    } else {
                        $(".page_btn_box .page_btn").eq(1).attr("href", "/logoDetail?seq="+nextSeq)
                    }
                }

                if(prevSeq > 0){
                    $(".page_btn_box .page_btn").eq(0).removeClass("disable");
                    if(prevSeq >= 1000){
                        $(".page_btn_box .page_btn").eq(0).attr("href", "/detail?seq="+prevSeq+"&logoType=")
                    } else {
                        $(".page_btn_box .page_btn").eq(0).attr("href", "/logoDetail?seq="+prevSeq)
                    }
                }
                
                $(".page_btn_box .page_btn").show();

            },
            error : function(e){
                console.log(e);
            }
        });
    });

    window.onload = function() {
        window.__splugin = SocialPlugIn_Core({
            "evKey"       : "logoproject",
            "serviceName" :"로고프로젝트",
            "sourceName" : "hanguel2018"
        });
    };

    window.addEventListener("load", function() {
        window.seViewer = new SmartEditorViewer();

        seViewer.bootstrap({
            SERVICE_ID: "logo",
            DEVICE_TYPE : "desktop",
            SERVICE_INAPP : false,
            MODULE_LOADING_TYPE : "normal",
            VIEWER_TYPE: "normal",
            MODULE_INJECTION_DATA: {
                VIDEO: {
                    ALLOW_SHARE_BUTTON_ALL: {
                        hasLink: false
                    },
                }
            }
        });
    });
</script>
